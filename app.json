{
  "expo": {
    "name": "AI Photo Finder",
    "slug": "aiphoto-finder",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons/icon.png",
    "scheme": "aiphotofinder",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/icons/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#121212"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.company.aiphotofinder"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/adaptive-icon.png",
        "backgroundColor": "#121212"
      },
      "package": "com.company.aiphotofinder",
      "versionCode": 2,
      "permissions": [
        "android.permission.READ_MEDIA_IMAGES",
        "android.permission.READ_EXTERNAL_STORAGE"
      ]
    },
    "plugins": [
      "expo-router",
      "expo-dev-client",
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 34,
            "targetSdkVersion": 34,
            "minSdkVersion": 24,
            "kotlinVersion": "1.9.24",
            "gradleProperties": {
              "org.gradle.jvmargs": "-Xmx6g -XX:+UseParallelGC -Dkotlin.daemon.jvm.options=-Xmx2g",
              "android.useAndroidX": "true",
              "android.enableJetifier": "true",
              "android.enableDexingArtifactTransform": "false"
            },
            "packagingOptions": {
              "pickFirst": [
                "**/libc++_shared.so",
                "**/libonnxruntime.so",
                "**/libonnxruntime4j_jni.so"
              ],
              "exclude": [
                "META-INF/LICENSE",
                "META-INF/DEPENDENCIES",
                "META-INF/INDEX.LIST",
                "META-INF/versions/**"
              ]
            }
          },
          "ios": {
            "deploymentTarget": "13.4"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "c5075ff0-cb71-47c5-81d7-f19778222b2b"
      }
    }
  }
}
